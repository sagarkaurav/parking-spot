<div class="container-fluid">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <%= form_tag admin_panel_do_checkout_path, method: :post do %>
      <div class="form-group">
        <label for="token">Ticket</label>
        <input type="text" class="form-control" name="token" placeholder="Ticket" value= <%= @token %>>
      </div>
      <div class="form-group">
        <label for="license">License Plate number</label>
        <input type="text" class="form-control" name="license" placeholder="License">
      </div>
      <div class="form-group">
        <input type="submit" value="Check out" class="btn btn-primary btn-block">
      </div>
      <% end %>
    </div>
  </div>
</div>
